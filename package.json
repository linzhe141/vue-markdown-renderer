{
  "name": "vue-mdr",
  "version": "0.0.11",
  "description": "Tranform markdown string to vue component with `mdast-> hast-> vue node`",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm -F playground run dev",
    "build": "tsc",
    "build:playground": "pnpm -F playground run build",
    "preview:playground": "pnpm -F playground run preview",
    "prettier-lint": "prettier --write ."
  },
  "keywords": [
    "vue",
    "markdown",
    "stream",
    "stream-markdown",
    "markdown to html",
    "markdown to vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linzhe141/vue-markdown-renderer"
  },
  "author": "linzhe141@qq.com",
  "license": "ISC",
  "packageManager": "pnpm@10.5.1",
  "devDependencies": {
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@shikijs/core": "^3.6.0",
    "@shikijs/langs": "^3.6.0",
    "@shikijs/themes": "^3.6.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.6.0",
    "shiki-stream": "^0.1.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3"
  }
